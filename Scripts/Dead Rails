-- Extra Tab
local extraTab = Window:CreateTab({
	Name = "Extra",
	Icon = "extension",
	ImageSource = "Material",
	ShowTitle = true
})

-- Fullbright Toggle
extraTab:CreateToggle({
	Name = "FullBright",
	Default = false,
	Callback = function(value)
		_G.FullBrightEnabled = value
		if not _G.FullBrightExecuted then
			_G.NormalLightingSettings = {
				Brightness = game.Lighting.Brightness,
				ClockTime = game.Lighting.ClockTime,
				FogEnd = game.Lighting.FogEnd,
				GlobalShadows = game.Lighting.GlobalShadows,
				Ambient = game.Lighting.Ambient
			}

			game.Lighting:GetPropertyChangedSignal("Brightness"):Connect(function()
				if _G.FullBrightEnabled then game.Lighting.Brightness = 1 end
			end)

			game.Lighting:GetPropertyChangedSignal("ClockTime"):Connect(function()
				if _G.FullBrightEnabled then game.Lighting.ClockTime = 12 end
			end)

			game.Lighting:GetPropertyChangedSignal("FogEnd"):Connect(function()
				if _G.FullBrightEnabled then game.Lighting.FogEnd = 786543 end
			end)

			game.Lighting:GetPropertyChangedSignal("GlobalShadows"):Connect(function()
				if _G.FullBrightEnabled then game.Lighting.GlobalShadows = false end
			end)

			game.Lighting:GetPropertyChangedSignal("Ambient"):Connect(function()
				if _G.FullBrightEnabled then game.Lighting.Ambient = Color3.fromRGB(178, 178, 178) end
			end)

			spawn(function()
				local lastState = _G.FullBrightEnabled
				while true do
					wait()
					if _G.FullBrightEnabled ~= lastState then
						if _G.FullBrightEnabled then
							game.Lighting.Brightness = 1
							game.Lighting.ClockTime = 12
							game.Lighting.FogEnd = 786543
							game.Lighting.GlobalShadows = false
							game.Lighting.Ambient = Color3.fromRGB(178, 178, 178)
						else
							game.Lighting.Brightness = _G.NormalLightingSettings.Brightness
							game.Lighting.ClockTime = _G.NormalLightingSettings.ClockTime
							game.Lighting.FogEnd = _G.NormalLightingSettings.FogEnd
							game.Lighting.GlobalShadows = _G.NormalLightingSettings.GlobalShadows
							game.Lighting.Ambient = _G.NormalLightingSettings.Ambient
						end
						lastState = _G.FullBrightEnabled
					end
				end
			end)

			_G.FullBrightExecuted = true
		end
	end
})

-- Air Weld Button
extraTab:CreateButton({
	Name = "Air Weld",
	Callback = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/ringtaa/NEWAIRWELD.github.io/refs/heads/main/weld.lua"))()
	end
})

-- Hitbox Button
extraTab:CreateButton({
	Name = "Zombie/Bandit Hitbox",
	Callback = function()
		local function CreateHitboxForEnemy(model)
			if not model:FindFirstChild("HumanoidRootPart") then return end

			local hitbox = Instance.new("Part")
			hitbox.Size = Vector3.new(20, 20, 20)
			hitbox.Transparency = 0.5
			hitbox.Color = Color3.fromRGB(0, 0, 255)
			hitbox.Material = Enum.Material.Neon
			hitbox.Anchored = true
			hitbox.CanCollide = false
			hitbox.Name = "EnemyHitbox"
			hitbox.Parent = model

			game:GetService("RunService").Heartbeat:Connect(function()
				if model and model:FindFirstChild("HumanoidRootPart") then
					hitbox.CFrame = model.HumanoidRootPart.CFrame
				end
			end)
		end

		local allowedNames = {
			["Model_Walker"] = true,
			["Model_Runner"] = true,
			["Model_Banker"] = true,
			["ZombieSheriff"] = true,
			["Outlaw"] = true
		}

		for _, obj in pairs(workspace:GetDescendants()) do
			if obj:IsA("Model") and allowedNames[obj.Name] then
				CreateHitboxForEnemy(obj)
			end
		end

		workspace.DescendantAdded:Connect(function(obj)
			if obj:IsA("Model") and allowedNames[obj.Name] then
				task.wait(0.5)
				CreateHitboxForEnemy(obj)
			end
		end)
	end
})

-- Noclip Toggle
extraTab:CreateToggle({
	Name = "Noclip",
	Default = false,
	Callback = function(state)
		local Plr = game:GetService("Players").LocalPlayer

		if state then
			_G.NoclipRunning = true
			_G.NoclipConnection = game:GetService("RunService").Stepped:Connect(function()
				for _, v in pairs(Plr.Character:GetDescendants()) do
					if v:IsA("BasePart") and _G.NoclipRunning then
						v.CanCollide = false
					end
				end
			end)
		else
			_G.NoclipRunning = false
			if _G.NoclipConnection then
				_G.NoclipConnection:Disconnect()
				_G.NoclipConnection = nil
			end
		end
	end
})

-- Instant Interact Toggle
extraTab:CreateToggle({
	Name = "Instant Interact",
	Default = false,
	Callback = function(state)
		if state then
			_G.InstantInteractConnection1 = workspace.DescendantAdded:Connect(function(descendant)
				if descendant:IsA("ProximityPrompt") then
					descendant.HoldDuration = 0
				end
			end)

			for _, prompt in pairs(workspace:GetDescendants()) do
				if prompt:IsA("ProximityPrompt") then
					prompt.HoldDuration = 0
				end
			end
		else
			if _G.InstantInteractConnection1 then
				_G.InstantInteractConnection1:Disconnect()
				_G.InstantInteractConnection1 = nil
			end
		end
	end
})

